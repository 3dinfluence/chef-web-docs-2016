msgid "help"
msgstr ""

#: ../../includes_ctl_manage/includes_ctl_manage_help.rst:6
# f3b1696e310240d9bd36cb5d27e2269b
msgid "The ``help`` subcommand is used to print a list of all available ``opscode-manage-ctl`` commands."
msgstr ""

#: ../source/ctl_manage.rst:20
# 785bfef40b074ca79d1a14e887de4ab4
msgid "reconfigure"
msgstr ""

#: ../../includes_ctl_manage/includes_ctl_manage_reconfigure.rst:6
# 8bd6e8cd4d0848598804b81926d200f8
msgid "The ``reconfigure`` subcommand is used when changes are made to the |manage rb| file to reconfigure the server. When changes are made to the |manage rb| file, they will not be applied to the |chef manage| configuration until after this command is run."
msgstr ""

#: ../source/ctl_manage.rst:24
# 30009a6612bc4d93986c1b308942dd39
msgid "show-config"
msgstr ""

#: ../../includes_ctl_manage/includes_ctl_manage_show_config.rst:6
# 31b5f10adf8f41319008dd1a1fee09c8
msgid "The ``show-config`` subcommand is used to view the configuration that will be generated by the ``reconfigure`` subcommand. This command is most useful in the early stages of a deployment to ensure that everything is built properly prior to installation."
msgstr ""

#: ../source/ctl_manage.rst:28
# bdfbabd72b114d0c8da1365a31fbf457
msgid "uninstall"
msgstr ""

#: ../../includes_ctl_manage/includes_ctl_manage_uninstall.rst:6
# 76d20eecf71a4c55853a138492f382b9
msgid "The ``uninstall`` subcommand is used to manage the hooks between |runit| and ``sysvinit`` or ``upstart``. This subcommand does not `uninstall the Chef management console <http://docs.chef.io/uninstall.html#chef-manage-title>`__."
msgstr ""

