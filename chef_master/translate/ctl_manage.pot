#: ../source/ctl_manage.rst:8
# de404830d0d84f7883f2a58d56410081
msgid "opscode-manage-ctl (executable)"
msgstr ""

#: ../../includes_ctl_manage/includes_ctl_manage.rst:5
# e0a275864bb249ffae5d16795440cfda
msgid "The |chef manage| includes a command-line utility named ``opscode-manage-ctl``. This command-line tool is used to reconfigure, cleanse (reset the |chef manage| to initial configuration settings), and uninstall the |chef manage|."
msgstr ""

#: ../source/ctl_manage.rst:13
# ab76a260c71e4abf82fc16048ee9e507
msgid "cleanse"
msgstr ""

#: ../../includes_ctl_manage/includes_ctl_manage_cleanse.rst:6
# 470a74b4a03b4d15ac2e84450b7dd6f1
msgid "The ``cleanse`` subcommand is used to re-set the |chef manage| to the state it was in prior to the first time the ``reconfigure`` subcommand is run. This command will destroy all data, configuration files, and logs. The software that was put on-disk by the package installation will remain; re-run ``opscode-manage-ctl reconfigure`` to recreate the default data and configuration files."
msgstr ""

