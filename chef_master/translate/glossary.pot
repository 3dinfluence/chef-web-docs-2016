#: ../source/glossary.rst:20
# 31d391cdd7684afa8585e86c23dd1731
msgid "**chef-apply**"
msgstr ""

#: ../source/glossary.rst:20
# 262523a041694104a858e41d18001357
msgid "A command-line tool that allows a single recipe to be run from the command line."
msgstr ""

#: ../source/glossary.rst:23
# 61a1678b60714df093e8a410273f946a
msgid "**chef-client**"
msgstr ""

#: ../source/glossary.rst:23
# 8ab01dbad3e84650829f24f59a81ef09
msgid "A command-line tool that that runs |chef|. Also, the name of |chef| as it is installed on a node."
msgstr ""

#: ../source/glossary.rst:26
# 21b30f620a5a471c9493e34a229b894e
msgid "**chef-repo**"
msgstr ""

#: ../source/glossary.rst:26
# a1456e4d99d74adea9a02a67e9da9f68
