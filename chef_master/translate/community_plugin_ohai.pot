msgstr ""

#: ../../includes_ohai/includes_ohai_plugins.rst:42
# 6e4f76434665469486c014ec246eeb06
msgid "`win32_software.rb <https://github.com/timops/ohai-plugins/blob/master/win32_software.rb>`_"
msgstr ""

