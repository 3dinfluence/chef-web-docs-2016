msgstr ""

#: ../../includes_ohai/includes_ohai_plugins.rst:26
# fea3460df6604f6999ebb5c5fbd123af
msgid "`network_ports.rb <https://github.com/agoddard/ohai-plugins/blob/master/plugins/network_ports.rb>`_"
