msgstr ""

#: ../../includes_ohai/includes_ohai_plugins.rst:40
# 1aa76db1138e46eeaf3b8319e1818976
msgid "`xenserver.rb <https://github.com/spheromak/cookbooks/blob/master/ohai/files/default/xenserver.rb>`_"
