msgid "Use the |resource package_freebsd| resource to manage packages for the |freebsd| platform."
msgstr ""

#: ../../includes_resources_common/includes_resources_common_platform_resources.rst:50
# a4ccb19c78244a1ab85b003ac3959a70
msgid "`gem_package <http://docs.chef.io/resource_gem_package.html>`_"
msgstr ""

